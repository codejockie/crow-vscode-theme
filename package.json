{
  "publisher": "codejockie",
  "name": "codejockie-dark-theme",
  "displayName": "Codejockie Dark Theme",
  "version": "0.2.6",
  "description": "Codejockie's Dark theme for VS Code",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejockie/vscode-theme-codejockie"
  },
  "bugs": {
    "url": "https://github.com/codejockie/vscode-theme-codejockie/issues"
  },
  "keywords": [
    "theme",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "./resources/icon.png",
  "files": [
    "themes"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Codejockie Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/codejockie-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec 'esno src/index.ts'",
    "vscode:prepublish": "npm run build",
    "update:key": "esno resources/update-keys.ts",
    "lint": "eslint . --cache --fix",
    "preview": "vsce package --no-dependencies",
    "release": "bumpp --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.2.1",
    "@types/node": "^20.4.8",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vscode/vsce": "^2.20.1",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "nodemon": "^3.0.1",
    "object-standard-path": "^0.3.0",
    "tinycolor2": "^1.6.0",
    "typescript": "^5.1.6"
  },
  "preview": true
}
